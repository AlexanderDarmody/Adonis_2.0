					<ProtectedString name="Source"><![CDATA[--[[
	
	Description: Handles various aspects of the UI object & Fires events which can be listened to.
	Author: Sceleratis
	Date: 2/5/2022
	
--]]


--// Prefab variables
local folder = script.Parent
local prefab = folder.Parent
local mainFrame = prefab:WaitForChild("ScrollingFrame")
local objects = {}

local pages = {}
local contents = {}
local pageSize = 100
local curPage = 1

local UIEvent = prefab:FindFirstChild("UIEvent") or Instance.new("BindableEvent")
local UIFunction = prefab:FindFirstChild("UIFunction") or Instance.new("BindableFunction")

local Player = game:GetService("Players").LocalPlayer
local InputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

--// Property handlers
local PropertyHandlers = {
	Contents = function(value)
		contents = value
		pages = Batchify(contents, pageSize)
		Populate()
		FireEvent("SetContents", value)
	end,
	
	Page = function(value)
		curPage = value
		Populate()
		FireEvent("SetPage", value)
	end,
	
	PageSize = function(value)
		pageSize = value
		pages = Batchify(contents, pageSize)
		Populate()
		FireEvent("SetPageSize", value)
	end,
}

--// Interface methods
local InterfaceMethods = {
	SetProperties = function(self, properties)
		for prop,val in pairs(properties) do
			local handler = PropertyHandlers[prop]
			if handler then
				handler(val)
			else
				prefab[prop] = val
			end
		end
	end,
	
	GetCurrentPage = function(self)
		return curPage
	end,
	
	GetPages = function(self)
		return pages
	end,
	
	GetContents = function(self)
		return contents
	end,
	
	SetContents = function(self, value)
		PropertyHandlers.Contents(value)
	end,
	
	SetPage = function(self, value)
		PropertyHandlers.Page(value)
	end,
	
	SetPageSize = function(self, value)
		PropertyHandlers.PageSize(value)
	end,
	
	SetSearchEnabled = function(self, value)
		SetSearchEnabled(value)
	end,
}

--// Event/function handlers
local function InterfaceHandler(method, ...)
	local found = InterfaceMethods[method]
	if found then
		return found(InterfaceMethods, ...)
	else
		warn("Window prefab interface method not found: ".. tostring(method))
	end
end

function FireEvent(type, ...)
	UIEvent:Fire(type, ...)
end


--// Helper functions
function IsInFrame(obj, x, y)
	local absPos = obj.AbsolutePosition
	local absSize = obj.AbsoluteSize

	return (x >= absPos.X and x <= absPos.X + absSize.X) and (y >= absPos.Y and y <= absPos.Y + absSize.Y)
end

function Batchify(tab, size)
	local batches = {}
	for b = 1, table.getn(tab), size do
		local batch = {}
		for i = b, b + size-1 do
			local item = tab[i]
			if item then
				table.insert(batch, item)
			else
				break
			end
		end
		table.insert(batches, batch)
	end
	return batches
end

function StringIsEmptyOrNull(str)
	return if str then string.match(str, "%S") ~= nil else false
end


--// Prefab-specific events/functionality
function Filter(str)
	local searchVal = objects.Search.TextBox.Text
	if not StringIsEmptyOrNull(searchVal) then
		if string.match(str, searchVal) then
			return true
		else
			return false
		end
	else
		return true
	end
end

function Populate()
	ClearContents()
	
	local page = pages[math.min(table.getn(pages), curPage)]
	
	if page then
		mainFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
		for i,v in ipairs(page) do
			local filter = v:GetAttribute("SearchKeywords")
			if not filter or (filter and Filter(filter)) then
				v.Parent = mainFrame
			end
		end
	end
	
	objects.PageNumber.Text = "Page: " .. curPage .. "/" .. table.getn(pages)
	FireEvent("Populate", page, curPage, pages)
end

function ClearContents()
	for i,v in ipairs(mainFrame:GetChildren()) do
		if v:IsA("GuiObject") then
			v.Parent = nil
		end
	end
	
	FireEvent("ClearContents")
end

function UpdateButtonsShown()
	local numPages = table.getn(pages)
	if numPages <= 1 then
		objects.NextPage.Visible = false
		objects.PreviousPage.Visible = false
		objects.PageNumber.Visible = false
		mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -35)
	else
		objects.NextPage.Visible = true
		objects.PreviousPage.Visible = true
		objects.PageNumber.Visible = true
		mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -65)
	end
end

function SetSearchEnabled(value)
	local numPages = table.getn(pages)
	
	objects.Search.Visible = value
	
	if objects.Search.Visible then
		mainFrame.Position = UDim2.new(mainFrame.Position.X.Scale, mainFrame.Position.X.Offset, 0, objects.Search.Visible.AbsoluteSize.Y)
		if numPages <= 1 then
			mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -35)
		else
			mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -65)
		end
	else
		mainFrame.Position = UDim2.new(mainFrame.Position.X.Scale, mainFrame.Position.X.Offset, 0, 0)
		if numPages <= 1 then
			mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -5)
		else
			mainFrame.Size = UDim2.new(mainFrame.Size.X.Scale, mainFrame.Size.X.Offset, 1, -30)
		end
	end
end

--// Input handlers
local function NextPage()
	local newPage = curPage + 1
	local numPages = table.getn(pages)
	
	UpdateButtonsShown()
	
	if numPages > 1 then
		if newPage > numPages then
			newPage = 1
		end
	end
	
	curPage = newPage
	Populate()
	FireEvent("NextPage", newPage)
end

local function PreviousPage()
	local newPage = curPage - 1
	local numPages = table.getn(pages)
	
	UpdateButtonsShown()
	
	if numPages > 1 then
		if newPage < 1 then
			newPage = numPages
		end
	end
	
	curPage = newPage
	Populate()
	FireEvent("PreviousPage", newPage)
end

--// Init
local function Init()
	UIEvent.Name = "UIEvent"
	UIFunction.Name = "UIFunction"

	for i,obj in ipairs(prefab:GetChildren()) do
		objects[obj.Name] = obj
	end
	
	objects.NextPage.Activated:Connect(NextPage)
	objects.PreviousPage.Activated:Connect(PreviousPage)
	
	UpdateButtonsShown()
	
	FireEvent("Init")
end

--// Events
UIFunction.OnInvoke = InterfaceHandler

--// Misc
Init()]]></ProtectedString>
