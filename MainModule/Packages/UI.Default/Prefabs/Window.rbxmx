					<ProtectedString name="Source"><![CDATA[--[[
	
	Description: Handles various aspects of the UI object & Fires events which can be listened to.
	Author: Sceleratis
	Date: 2/5/2022
	
--]]


--// Special properties
local MinSize = { 200, 100 }
local MaxSize = { 1000, 1000 }
local CloseFadeTime = 0.25

--// Prefab variables
local folder = script.Parent
local prefab = folder.Parent
local frame = prefab:WaitForChild("Frame")
local titleBar = frame:WaitForChild("TitleBar")
local originalDisplayOrder = prefab.DisplayOrder

local UIEvent = prefab:FindFirstChild("UIEvent") or Instance.new("BindableEvent")
local UIFunction = prefab:FindFirstChild("UIFunction") or Instance.new("BindableFunction")

local misc = {}
local special = {}
local objects = { MainFrame = frame }
local dragging = false
local resizing = false
local minimized = false
local refreshing = false
local hideSizing = false

local Player = game:GetService("Players").LocalPlayer
local InputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local RefreshSpinnerTweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, -1, false, 0)
local RefreshSpinnerTweenProperties = { Rotation = -360 }
local RefreshSpinnerTween

local HideTweenInfo = TweenInfo.new(0.25, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0)
local HideTween = false

local CloseTweenInfo = TweenInfo.new(CloseFadeTime, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0)

--// Property handlers
local PropertyHandlers = {
	Title = function(value)
		objects.Title.Text = value
	end,
	
	MinSize = function(value)
		MinSize = value
	end,
	
	MaxSize = function(value)
		MaxSize = value
	end,
	
	RefreshHandler = function(value)
		special.RefreshHandler = value
		SetButtonEnabled(objects.Refresh, true)
	end,
}

--// Interface methods
local InterfaceMethods = {
	SetProperties = function(self, properties)
		for prop,val in pairs(properties) do
			local handler = PropertyHandlers[prop]
			if handler then
				handler(val)
			else
				prefab[prop] = val
			end
		end
	end,

	GetContentFrame = function()
		return objects.Content
	end,
	
	SetButtonsEnabled = function(list)
		for b, enabled in ipairs(list) do
			local b = objects[b]
			if b then
				SetButtonEnabled(b, enabled)
			end
		end
	end,
	
	Close = DoClose,
	Hide = DoHide,
	Refresh = DoRefresh,
}

--// Event/function handlers
local function InterfaceHandler(method, ...)
	local found = InterfaceMethods[method]
	if found then
		return found(InterfaceMethods, ...)
	else
		warn("Window prefab interface method not found: ".. tostring(method))
	end
end

local function FireEvent(type, ...)
	UIEvent:Fire(type, ...)
end


--// Helper functions
function SetButtonEnabled(button, bool)
	button.Visible = bool
	
	RefreshButtons()
end

function RefreshButtons()
	local enabled = {}
	for name,obj in pairs({
		objects.Close, --// Order here also determines the order they'll appear in
		objects.Hide,
		objects.Refresh
	}) do
		if obj:IsA("TextButton") and obj.Visible then
			table.insert(enabled, obj)
		end
	end
	
	for i,obj in ipairs(enabled) do
		obj.Position = UDim2.new(1, -30 * i, 0, 5)
	end
end

function GetActiveWindows()
	local windows = {}
	local windowGroup = prefab:GetAttribute("AdonisWindowGroup") or 1
	for i,v in ipairs(Player.PlayerGui:GetChildren()) do
		if v:GetAttribute("AdonisWindowGroup") == windowGroup then
			table.insert(windows, v)
		end
	end
	return windows
end

function BringToFront()
	local maxOrder = originalDisplayOrder

	for i,v in ipairs(GetActiveWindows()) do
		if not v:GetAttribute("WindowFocus_OriginalDisplayOrder") then
			v:SetAttribute("WindowFocus_OriginalDisplayOrder", v.DisplayOrder)
		end

		local origOrder = v:GetAttribute("WindowFocus_OriginalDisplayOrder")
		v.DisplayOrder = origOrder - 1

		if v.DisplayOrder > maxOrder then
			maxOrder = v.DisplayOrder
		end
	end

	prefab.DisplayOrder = maxOrder + 1
end

function IsInFrame(obj, x, y)
	local absPos = obj.AbsolutePosition
	local absSize = obj.AbsoluteSize

	return (x >= absPos.X and x <= absPos.X + absSize.X) and (y >= absPos.Y and y <= absPos.Y + absSize.Y)
end

function IsInWindow(x, y)
	if prefab.Enabled and IsInFrame(prefab, x, y) then
		for i,v in ipairs(GetActiveWindows()) do
			if v.Enabled and IsInFrame(v, x, y) and v.DisplayOrder > prefab.DisplayOrder then
				return false
			end
		end

		return true
	end
end

function IsResizeRequest(x, y)
	for name,obj in pairs(objects) do
		local zone = name:match("Resize_(.*)")
		if zone and IsInFrame(obj, x, y) then
			return zone
		end
	end
end

--// Prefab-specific events/functionality
function DoClose()
	FireEvent("Close")
	for name,obj in pairs(objects) do
		if obj:IsA("GuiObject") then
			TweenService:Create(obj, CloseTweenInfo, {
				Transparency = 1
			}):Play()
		end
	end
	
	wait(CloseFadeTime)
	prefab:Destroy()
end

local function HideTweenComplete(playbackState)
	hideSizing = false
	HideTween = false
	
	if playbackState == Enum.PlaybackState.Completed then
		minimized = not minimized
		objects.Content.Visible = not minimized
		FireEvent("Minimized", minimized)
	end
end

function DoHide(forced)
	local override = if forced ~= nil then true else false
	
	if not hideSizing then
		hideSizing = true
		
		if (override and not forced) or (not override and minimized) then
			frame.ClipsDescendants = false
			
			HideTween = TweenService:Create(frame, HideTweenInfo, {
				Size = UDim2.new(0, misc.PreHideFrameX or MinSize[1], 0, misc.PreHideFrameY or MinSize[2])
			})

			objects.Content.Visible = true
			HideTween.Completed:Connect(HideTweenComplete)
			HideTween:Play()
		elseif (override and forced) or (not override and not minimized) then
			misc.PreHideFrameX = frame.AbsoluteSize.X
			misc.PreHideFrameY = frame.AbsoluteSize.Y
			
			frame.ClipsDescendants = true
			
			HideTween = TweenService:Create(frame, HideTweenInfo, {
				Size = UDim2.new(0, MinSize[1], 0, 45)
			})

			HideTween.Completed:Connect(HideTweenComplete)
			HideTween:Play()
		end
	end
end

function DoRefresh(forced)
	if forced or not refreshing then
		RefreshSpinnerTween:Play()
		if special.RefreshHandler then 
			local r,e = pcall(special.RefreshHandler)
			if not r then
				warn("RefreshHandler Failed:", e)
			end
		end
		wait(1)
		RefreshSpinnerTween:Cancel()
		FireEvent("Refreshed", forced)
	end
end

--// Input handlers
local function InputBegan(input, gameHandled)
	if not gameHandled and prefab.Enabled and (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
		local x,y = input.Position.X,input.Position.Y
		if IsInWindow(x, y) then
			BringToFront()

			resizing = not minimized and IsResizeRequest(x, y)

			if resizing then
				misc.FramePosX = frame.AbsolutePosition.X
				misc.FramePosY = frame.AbsolutePosition.Y

				misc.FrameSizeX = frame.AbsoluteSize.X
				misc.FrameSizeY = frame.AbsoluteSize.Y

				misc.FrameDragStartX = x - frame.AbsolutePosition.X
				misc.FrameDragStartY = y - frame.AbsolutePosition.Y
			elseif IsInFrame(titleBar, x, y) then
				misc.FrameDragStartX = x - frame.AbsolutePosition.X
				misc.FrameDragStartY = y - frame.AbsolutePosition.Y

				dragging = true
			end
			
			FireEvent("InputBegan", input, gameHandled)
		end
	end
end

local function InputChanged(input, gameHandled)
	if prefab.Enabled and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
		local x,y = input.Position.X,input.Position.Y

		if dragging then
			local newX = (x - misc.FrameDragStartX)
			local newY = (y - misc.FrameDragStartY)

			frame.Position = UDim2.new(0, newX, 0, newY)
		elseif resizing then
			local newPos = UDim2.new(0, frame.AbsolutePosition.X, 0, frame.AbsolutePosition.Y)
			local sizeX = misc.FrameSizeX
			local sizeY = misc.FrameSizeY
			local posX = misc.FramePosX
			local posY = misc.FramePosY

			local moveX = false
			local moveY = false

			if resizing == "UpperRight" then
				sizeX = (x - posX) + 3
				sizeY = (posY - y) + sizeY - 1
				moveY = true
			elseif resizing == "UpperLeft" then
				sizeX = (posX - x) + sizeX -1
				sizeY = (posY - y) + sizeY -1
				moveY = true
				moveX = true
			elseif resizing == "Right" then
				sizeX = (x - posX) + 3
				sizeY = sizeY
			elseif resizing == "Left" then
				sizeX = (posX - x) + sizeX + 3
				moveX = true
			elseif resizing == "LowerRight" then
				sizeX = (x - posX) + 3
				sizeY = (y - posY) + 3
			elseif resizing == "LowerLeft" then
				sizeX = (posX - x) + sizeX + 3
				sizeY = (y - posY) + 3
				moveX = true
			elseif resizing == "Top" then
				sizeY = (posY - y) + sizeY - 1
				moveY = true
			elseif resizing == "Bottom" then
				sizeX = sizeX
				sizeY = (y - posY) + 3
			end

			if sizeX < MinSize[1] then sizeX = MinSize[1] end
			if sizeY < MinSize[2] then sizeY = MinSize[2] end
			if sizeX > MaxSize[1] then sizeX = MaxSize[1] end
			if sizeY > MaxSize[2] then sizeY = MaxSize[2] end

			if moveX then
				newPos = UDim2.new(0, (posX + misc.FrameSizeX) - sizeX, 0, newPos.Y.Offset)
			end

			if moveY then
				newPos  = UDim2.new(0, newPos.X.Offset, 0, (posY + misc.FrameSizeY) - sizeY)
			end

			frame.Position = newPos
			frame.Size = UDim2.new(0, sizeX, 0, sizeY)
			
			--[[if not objects.Title.TextFits then
				objects.Title.Visible = false
			else
				objects.Title.Visible = true
			end--]]
		end
	end
end

local function InputEnded(input, gameHandled)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
		local x,y = input.Position.X,input.Position.Y

		dragging = false
		resizing = false
		
		FireEvent("InputEnded", input, gameHandled)
	end
end


--// Init
local function Init()
	UIEvent.Name = "UIEvent"
	UIFunction.Name = "UIFunction"

	for i,obj in ipairs(frame:GetChildren()) do
		objects[obj.Name] = obj
	end
	
	objects.Close.Activated:Connect(function() DoClose() end)
	objects.Hide.Activated:Connect(function() DoHide() end)
	objects.Refresh.Activated:Connect(function() DoRefresh() end)
	
	RefreshSpinnerTween = TweenService:Create(objects.Refresh:WaitForChild("Spinner"), RefreshSpinnerTweenInfo, RefreshSpinnerTweenProperties)
	
	SetButtonEnabled(objects.Refresh, false)
	FireEvent("Init")
end

